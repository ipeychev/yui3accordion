    <span class="nx">_createHeader</span><span class="o">:</span> <span class="k">function</span><span class="p">(){</span>
        <span class="k">var</span> <span class="nx">_strings</span><span class="o">,</span> <span class="nx">_html</span><span class="o">,</span> <span class="nx">_node</span><span class="o">,</span> <span class="nx">_closable</span><span class="o">;</span>

